[global]
toplevels=["appmenu"]
is-dark=false
is-custom=false
logout-command='lxsession-logout'
shutdown-command='lxsession-logout'

[appmenu]
edge='top'
monitor=-1
height=26
strut=true
icon-size=22
applets=["menumodel","appmenu-valapanel","separator","xembed","sntray","clock"]

[menumodel]
plugin-type='menumodel'
position=0
is-menu-bar=false

[appmenu-valapanel]
plugin-type='appmenu-valapanel'
position=1

[separator]
plugin-type='separator'
position=2
is-expanded=true
show-separator=false

[xembed]
plugin-type='xembed'
position=3

[sntray]
plugin-type='sntray-valapanel'
is-expanded=false
position=4
show-application-status=true
show-communications=true
show-system=true
show-hardware=true
index-override={'vala-panel-extras-keyboard': <-90>, 'nm-applet': <10>, 'vala-panel-extras-weather': <100>, 'indicator-weather': <100>, 'vala-panel-extras-volume': <90>, 'vala-panel-extras-battery': <-10>}
indicator-size=22
symbolic-icons=false
show-passive=true
filter-override={}

[clock]
plugin-type='clock'
position=5
clock-format='%R:%S'

